section#arrys
	h4 Operacje na tablicach
	p Obiekt typu Array posiada w swoim prototypie wiele metod przydatych do wykonywnia operacji na danych.
	p Każda taka funkcja przyjmuje callback, który jest wywoływany dla każdego elementu talbicy.
	p Operacje te zwracają nową tablicę i nie modyfikują istniejącej.
section
	h4 Podstawowe funckje na tablicach
	pre.fragment.fade-in.small-pre.margined(data-fragment-index='1')
		code.javascript.hljs(data-noescape)
			| const data = [1,2,3,4];
			| data.forEach((val,index) => console.log(`Value ${val}, is on index ${index}));
	pre.fragment.fade-in.small-pre.margined(data-fragment-index='2')
		code.javascript.hljs(data-noescape)
			| const data = [1,2,3,4];
			| data.map((val,index) => val * index);
			span.fragment.fade-out.display-none(data-fragment-index='3') // ???
			span.fragment.fade-in(data-fragment-index='3') // [0, 2, 6, 12]
	pre.fragment.fade-in.small-pre.margined(data-fragment-index='4')
		code.javascript.hljs(data-noescape)
			| const data = [{}, '', 15, null];
			| const callback = (val) => typeof val === "number" || typeof val === "undefined";
			| data.filter(callback);
			span.fragment.fade-out.display-none(data-fragment-index='5') // ???
			span.fragment.fade-in(data-fragment-index='5') // [15]
	pre.fragment.fade-in.small-pre.margined(data-fragment-index='6')
		code.javascript.hljs(data-noescape)
			| const data = ["Java", "JavaScript", "StarterKit", "Capgemini"];
			| data.find(val => val.length > 4);
			span.fragment.fade-out.display-none(data-fragment-index='7') // ???
			span.fragment.fade-in(data-fragment-index='7') //  "JavaScript"
section
	h4 Array.prototype.reduce
	pre.fragment.fade-in.small-pre.margined(data-fragment-index='1')
		code.javascript.hljs(data-noescape)
			| const data = [1,2,3,4];
			| data.reduce((acc, curr, index) => {
			| 	return acc+curr;
			| }, 0);
			span.fragment.fade-out.display-none(data-fragment-index='2') // ???
			span.fragment.fade-in(data-fragment-index='2') //  10
	pre.fragment.fade-in.small-pre.margined(data-fragment-index='3')
		code.javascript.hljs(data-noescape)
			| const data = [1,2,3,4,5,6];
			| data.reduce((acc, curr, index) => {
			| 	return curr % 2 ? [...acc, curr] : acc;
			| }, []);
			span.fragment.fade-out.display-none(data-fragment-index='4') // ???
			span.fragment.fade-in(data-fragment-index='4') //  [1, 3, 5]
section
	h4 Destrukturyzacja tablic
	pre.fragment.fade-in.small-pre.margined(data-fragment-index='1')
		code.javascript.hljs(data-noescape)
			| const data = [1,2,3]
			| const test = [4,5,6];
			| [...test, ...data]
			span.fragment.fade-out.display-none(data-fragment-index='2') // ???
			span.fragment.fade-in(data-fragment-index='2') //  [4, 5, 6, 1, 2, 3]
	pre.fragment.fade-in.small-pre.margined(data-fragment-index='3')
		code.javascript.hljs(data-noescape)
			| const position = [52, 35];
			| const [x, y] = position;
			span.fragment.fade-out.display-none(data-fragment-index='4') // ???
			span.fragment.fade-in(data-fragment-index='4') //  x = 52; y = 35
